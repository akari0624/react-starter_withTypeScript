{
  "name": "react_boilerplate_simple_starter",
  "version": "1.2.0",
  "description": "react+redux boilerplate simple starter",
  "main": "index.ts",
  "repository": "https://github.com/akari0624/react-starter_withTypeScript",
  "scripts": {
    "dev": "webpack-dev-server --public-host --config ./webpack_config/webpack.config.dev.js --mode development ",
    "devWithBabel": "cross-env NODE_ENV=withBabel webpack-dev-server --public-host --config ./webpack_config/webpack.config.dev.js --mode development ",
    "fastbuild_dev": "webpack-dev-server --public-host --config ./webpack_config/webpack.config.dev.fastbuild.js --mode development ",
    "fastbuild_devWithBabel": "cross-env  NODE_ENV=withBabel webpack-dev-server --public-host --config ./webpack_config/webpack.config.dev.fastbuild.js --mode development ",
    "dist": "npm run clean && webpack --config ./webpack_config/webpack.config.prod.js --progress --bail --env dist -p",
    "distWithBabel": "cross-env  NODE_ENV=withBabel npm run clean && webpack --config ./webpack_config/webpack.config.prod.js --progress --bail --env dist -p",
    "deploy": "npm run dist && gh-pages -d dist",
    "deployWithBabel": "npm run distWithBabel && gh-pages -d dist",
    "test": "jest --verbose",
    "test:watch": "jest --watch --verbose",
    "clean": "rimraf dist/*",
    "bundle_analyze": "npm run clean && cross-env BUNDLE_ANALYZER=bundle_analyzer webpack --config ./webpack_config/webpack.config.prod.js --progress --bail --env dist -p"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
